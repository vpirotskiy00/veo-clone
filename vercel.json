{
  "buildCommand": "npm run build:vercel",
  "installCommand": "bun install",
  "framework": "nextjs",
  "functions": {
    "app/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_VERSION": "18.17.0"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "CI": "true",
      "DISABLE_FILE_TRACING": "1"
    }
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false
}
