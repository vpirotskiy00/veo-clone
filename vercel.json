{
  "framework": "nextjs",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "env": {
    "NODE_VERSION": "18.17.0"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "CI": "true"
    }
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false
}
